<?xml version="1.0" encoding="UTF-8"?>
<package name="de.chrihis.wcf.linkList.im-exporter" xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com/XSD/package.xsd">
	<packageinformation>
		<packagename>Link Sammlung: Im- und Exporter</packagename>
		<packagedescription>Sichern Sie Ihre Links und laden Sie sie ggf. spielend einfach wieder hoch - beide Aktionen sind mit weniger als 10 Klicks gemacht.</packagedescription>
		<version>1.0.0 Beta 1</version>
		<date>2010-12-10</date>
		<plugin>com.woltlab.wcf.data.page</plugin>
		<standalone>0</standalone>
		<isunique>1</isunique>
	</packageinformation>
  
	<authorinformation>
		<author>Christoph H. (Chrihis)</author>
		<authorurl><![CDATA[http://community.woltlab.com/db/index.php?page=User&userID=1339032]]></authorurl>
	</authorinformation>
  
	<requiredpackages>
		<requiredpackage minversion="1.1.0">com.woltlab.wcf</requiredpackage>
		<requiredpackage minversion="1.1.0 Alpha 1">de.chrihis.wcf.linkList</requiredpackage>
	</requiredpackages>
	
	<instructions type="install">
		<!-- files -->
		<files>files.tar</files>
		<acptemplates>acptemplates.tar</acptemplates>
		<script>acp/install_linkListImExporterPlugin.php</script>
		
		<!-- config -->
		<acpmenu>acpmenu.xml</acpmenu>
		<groupoptions>groupoptions.xml</groupoptions>
		<eventlistener>eventlistener.xml</eventlistener>
		
		<!-- languages -->
		<languages languagecode="de">de.xml</languages>
		<languages languagecode="de-informal">de-informal.xml</languages>
	</instructions>
</package>
